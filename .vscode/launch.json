{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "C++ Debug (External Terminal)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/ryve",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "make",
					"text": "make",
					"type": "shell",
					"cwd": "${workspaceFolder}"
				}
			],
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}
